{
  "name": "Request Block",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "My outstanding extension",
  "author": "David Gamboa",
  "permissions":["webRequest","webRequestBlocking", "<all_urls>", "storage","tabs"],
  "homepage_url": "https://github.com/HasselGR/request-block",
  "icons": {
    "128": "images/icon.png"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
